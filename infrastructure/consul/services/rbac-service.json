{
  "service": {
    "id": "rbac-service",
    "name": "rbac-service",
    "tags": [
      "auth",
      "rbac",
      "microservice",
      "nodejs",
      "express"
    ],
    "address": "aiot-rbac-service",
    "port": 3051,
    "meta": {
      "version": "1.0.0",
      "env": "development",
      "protocols": "http,grpc"
    },
    "check": {
      "id": "rbac-service-health",
      "name": "RBAC Service Health Check",
      "http": "http://aiot-rbac-service:3051/health",
      "interval": "10s",
      "timeout": "3s",
      "deregister_critical_service_after": "30s"
    }
  }
}