{
  "service": {
    "id": "general-service",
    "name": "general-service",
    "tags": [
      "general",
      "documentation",
      "microservice",
      "nodejs",
      "express"
    ],
    "address": "aiot-general-service",
    "port": 3003,
    "meta": {
      "version": "1.0.0",
      "env": "development",
      "protocols": "http,grpc"
    },
    "check": {
      "id": "general-service-health",
      "name": "General Service Health Check",
      "http": "http://aiot-general-service:3003/health",
      "interval": "10s",
      "timeout": "3s",
      "deregister_critical_service_after": "30s"
    },
    "checks": [
      {
        "id": "general-service-api",
        "name": "General API Check",
        "http": "http://aiot-general-service:3003/api/health",
        "interval": "30s",
        "timeout": "5s"
      },
      {
        "id": "general-service-grpc",
        "name": "General gRPC Check",
        "grpc": "aiot-general-service:50053",
        "interval": "30s",
        "timeout": "5s"
      }
    ],
    "weights": {
      "passing": 10,
      "warning": 1
    }
  }
}