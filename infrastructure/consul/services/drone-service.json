{
  "service": {
    "id": "drone-service",
    "name": "drone-service",
    "tags": [
      "drone",
      "iot",
      "api",
      "microservice",
      "nodejs",
      "grpc"
    ],
    "address": "aiot-drone-service",
    "port": 3052,
    "meta": {
      "version": "1.0.0",
      "env": "development",
      "protocols": "grpc,http"
    },
    "check": {
      "id": "drone-service-health",
      "name": "Drone Service Health Check",
      "http": "http://aiot-drone-service:3052/health",
      "interval": "10s",
      "timeout": "3s",
      "deregister_critical_service_after": "30s"
    }
  }
}