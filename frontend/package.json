{
  "name": "fe",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.27.6",
    "@reduxjs/toolkit": "^2.8.2",
    "@tanstack/react-query": "^5.83.0",
    "@types/google.maps": "^3.58.1",
    "axios": "^1.10.0",
    "clsx": "^2.1.1",
    "lodash": "^4.17.21",
    "loglevel": "^1.9.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.23.0",
    "socket.io-client": "^4.8.1",
    "tslib": "^2.8.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.83.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.4",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.0",
    "cypress": "^15.0.0",
    "happy-dom": "^18.0.1",
    "http-server": "^14.1.1",
    "jsdom": "^26.1.0",
    "msw": "^2.10.5",
    "postcss": "^8.4.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.89.2",
    "tailwindcss": "^3.4.0",
    "typedoc": "^0.28.7",
    "typescript": "^5.4.0",
    "vite": "^5.2.0",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "docs:generate": "typedoc",
    "docs:serve": "npx http-server docs -p 3002 -o",
    "docs:watch": "typedoc --watch",
    "docs:clean": "rm -rf docs",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run src/test/unit",
    "test:integration": "vitest run src/test/integration",
    "test:e2e": "cypress run",
    "test:e2e:headed": "cypress open",
    "test:e2e:chrome": "cypress run --browser chrome",
    "test:e2e:firefox": "cypress run --browser firefox",
    "test:e2e:edge": "cypress run --browser edge",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:install": "cypress install",
    "test:sidebar-routes": "cypress run --spec 'cypress/e2e/sidebar-routes.cy.ts'",
    "test:selenium": "echo '⚠️ Selenium tests deprecated. Use: npm run test:sidebar-routes'",
    "test:selenium:headless": "echo '⚠️ Selenium tests deprecated. Use: npm run cypress:run'",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "vitest run --coverage --reporter=junit --outputFile=coverage/junit.xml"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
