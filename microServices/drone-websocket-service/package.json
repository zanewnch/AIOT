{
  "name": "drone-websocket-service",
  "version": "1.0.0",
  "description": "AIOT Drone WebSocket Service",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node --import tsx/esm src/server.ts",
    "dev": "npx nodemon --exec \"node --import tsx/esm\" src/server.ts",
    "build": "npx tsc",
    "build:watch": "tsc --watch",
    "serve": "node ./dist/server.js",
    "test": "jest --watchAll",
    "docs:generate": "typedoc",
    "docs:serve": "npx http-server docs -p 8001 -o",
    "docs:watch": "typedoc --watch",
    "docs:clean": "rm -rf docs",
    "docs:compile-scss": "sass src/styles/typedoc-custom-flex.scss:public/typedoc-custom.css",
    "docs:watch-scss": "sass --watch src/styles/typedoc-custom-flex.scss:public/typedoc-custom.css"
  },
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/socket.io": "^3.0.1",
    "aiot-shared-packages": "file:../../aiot-shared-packages",
    "axios": "^1.11.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "inversify": "^7.7.1",
    "pg": "^8.8.0",
    "redis": "^5.6.0",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "socket.io": "^4.8.1",
    "tsx": "^4.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.9",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.7",
    "typescript": "^5.8.3"
  }
}
