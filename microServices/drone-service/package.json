{
  "name": "drone-service",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "concurrently \"node --import tsx/esm src/server.ts\" \"node --import tsx/esm src/server-grpc.ts\"",
    "start:http": "node --import tsx/esm src/server.ts",
    "start:grpc": "node --import tsx/esm src/server-grpc.ts",
    "dev": "concurrently \"npx nodemon --exec 'node --import tsx/esm' src/server.ts\" \"npx nodemon --exec 'node --import tsx/esm' src/server-grpc.ts\"",
    "dev:http": "npx nodemon --exec \"node --import tsx/esm\" src/server.ts",
    "dev:grpc": "npx nodemon --exec \"node --import tsx/esm\" src/server-grpc.ts",
    "build": "npx tsc",
    "build:watch": "tsc --watch",
    "serve": "node ./dist/server.js",
    "test": "jest --watchAll",
    "docs:generate": "typedoc",
    "docs:serve": "npx http-server docs -p 8001 -o",
    "docs:watch": "typedoc --watch",
    "docs:clean": "rm -rf docs",
    "docs:compile-scss": "sass src/styles/typedoc-custom-flex.scss:public/typedoc-custom.css",
    "docs:watch-scss": "sass --watch src/styles/typedoc-custom-flex.scss:public/typedoc-custom.css"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.12.2",
    "@grpc/proto-loader": "^0.7.13",
    "@types/amqplib": "^0.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/ejs": "^3.1.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.15.5",
    "@types/socket.io": "^3.0.1",
    "aiot-shared-packages": "git+https://github.com/zanewnch/aiot-shared-packages.git",
    "amqplib": "^0.10.3",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "concurrently": "^8.2.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "grpc-tools": "^1.12.4",
    "http-errors": "~1.6.3",
    "inversify": "^7.7.0",
    "jade": "^0.29.0",
    "js-yaml": "^3.14.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.17.0",
    "mongoose": "^8.16.0",
    "morgan": "^1.10.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "pug": "^3.0.3",
    "redis": "^5.6.0",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "tsx": "^4.0.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.3",
    "@types/http-errors": "^2.0.5",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/mongodb": "^4.0.6",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.9",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "sass": "^1.89.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.7",
    "typedoc-theme-hierarchy": "^6.0.0",
    "typescript": "^5.8.3"
  }
}
