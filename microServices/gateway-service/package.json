{
  "name": "gateway-service",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node --import tsx/esm src/server.ts",
    "dev": "npx nodemon --exec \"node --import tsx/esm\" src/server.ts",
    "build": "npx tsc",
    "build:watch": "tsc --watch",
    "serve": "node ./dist/server.js",
    "test": "jest --watchAll",
    "docs:generate": "typedoc",
    "docs:serve": "npx http-server docs -p 8005 -o",
    "docs:watch": "typedoc --watch",
    "docs:clean": "rm -rf docs"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.12.2",
    "@grpc/proto-loader": "^0.7.13",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.11.0",
    "concurrently": "^8.2.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "http-errors": "~1.6.3",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^3.0.0",
    "inversify": "^7.7.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "redis": "^5.6.0",
    "reflect-metadata": "^0.2.2",
    "tsx": "^4.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.3",
    "@types/http-errors": "^2.0.5",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.11",
    "@types/supertest": "^6.0.3",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.7",
    "typescript": "^5.9.2"
  }
}
